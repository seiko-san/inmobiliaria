<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Controlador</servlet-name>
        <servlet-class>controlador.Controlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PropiedadesServlet</servlet-name>
        <servlet-class>Servlet.PropiedadesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NuevaPropiedad</servlet-name>
        <servlet-class>Servlet.NuevaPropiedad</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearUsuario</servlet-name>
        <servlet-class>controlador.CrearUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registro_Propietario</servlet-name>
        <servlet-class>controlador.Registro_Propietario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Controlador</servlet-name>
        <url-pattern>/Controlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PropiedadesServlet</servlet-name>
        <url-pattern>/PropiedadesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NuevaPropiedad</servlet-name>
        <url-pattern>/crearpropiedad</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registro_Propietario</servlet-name>
        <url-pattern>/Registro_Propietario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
